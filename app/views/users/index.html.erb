<div>
  <h2>Users</h2>
     <table class="table">
       <thead>
         <tr>
           <th>image</th>
           <th>name</th>
         </tr>
       </thead>
       <% @users.each do |user| %>
           <tbody>
             <tr>
               <th><%= image_tag user.get_profile_image(80,80) %></th>
               <th><%= user.name %></th>
               <th><%= link_to "Show", user_path(user.id)%></th>
             </tr>
           </tbody>
        <% end %>
     </table>
</div>